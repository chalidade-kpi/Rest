{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "yajra/laravel-oci8": "^5",
        "firebase/php-jwt": "^5.0",
        "tymon/jwt-auth": "^0.5.12"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helper/FileUpload.php",
            "app/Helper/RequestBooking.php",
            "app/Helper/BillingEngine.php",
            "app/Helper/UserAndRoleManagemnt.php"
